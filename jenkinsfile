pipeline {
	agent any

	tools {
		jdk 'java-17'
	}

	stages {

		stage('Build'){
			steps {
				echo "Build stage"
				bat "mvn clean install -DskipTests"
			}
		}

		stage('Test'){
			steps{
				echo "Test stage"
				bat "mvn test"
			}
		}

		stage('Deploy') {
			steps {
			    echo "Deploy stage"
			}
		}
	}
}